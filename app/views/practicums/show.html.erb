<%- model_class = Practicum -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:id_practicum) %>:</strong></dt>
  <dd><%= @practicum.id_practicum %></dd>
  <dt><strong><%= model_class.human_attribute_name(:practicum_name) %>:</strong></dt>
  <dd><%= @practicum.practicum_name %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              practicums_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_practicum_path(@practicum), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              practicum_path(@practicum),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
